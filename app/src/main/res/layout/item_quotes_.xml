<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout        xmlns:android="http://schemas.android.com/apk/res/android"        xmlns:tools="http://schemas.android.com/tools"        xmlns:app="http://schemas.android.com/apk/res-auto"        android:orientation="vertical"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:padding="@dimen/_4sdp">    <androidx.recyclerview.widget.RecyclerView            android:id="@+id/listQuoteImages"            android:layout_width="0dp"            android:layout_height="@dimen/_120sdp"            android:layout_marginTop="@dimen/_4sdp"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintHorizontal_chainStyle="spread_inside"            app:layout_constraintEnd_toStartOf="@+id/txtAddress"            app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"            app:spanCount="2"/>    <androidx.appcompat.widget.AppCompatImageView            android:id="@+id/imgProductPlaceHolder"            android:layout_width="0dp"            android:layout_height="@dimen/_120sdp"            android:layout_marginTop="@dimen/_4sdp"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            android:src="@drawable/img_anco_products"            android:visibility="gone"            app:layout_constraintHorizontal_chainStyle="spread_inside"            app:layout_constraintEnd_toStartOf="@+id/txtAddress"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtDate"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/listQuoteImages"            app:layout_constraintEnd_toStartOf="@+id/txtResend"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintBottom_toTopOf="@+id/txtAddress"            android:text="@string/quote_date"            android:textColor="@android:color/black"            android:layout_marginStart="@dimen/_4sdp"            app:layout_constraintVertical_chainStyle="spread"            android:textSize="@dimen/_12ssp"            android:fontFamily="@font/sf_pro_text_regular"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtAddress"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/listQuoteImages"            app:layout_constraintEnd_toStartOf="@+id/txtResend"            app:layout_constraintBottom_toTopOf="@+id/txtQuoteNumber"            app:layout_constraintTop_toBottomOf="@+id/txtDate"            tools:text="1 Quote St ,\nWindsor , VIC, 311"            android:textColor="@android:color/black"            android:layout_marginStart="@dimen/_4sdp"            android:textSize="@dimen/_12ssp"            android:fontFamily="@font/sf_pro_text_regular"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtQuoteNumber"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/listQuoteImages"            app:layout_constraintEnd_toStartOf="@+id/txtResend"            app:layout_constraintTop_toBottomOf="@+id/txtAddress"            app:layout_constraintBottom_toTopOf="@+id/txtQuotePrice"            android:text="@string/quote_number"            android:textColor="@android:color/black"            android:layout_marginStart="@dimen/_4sdp"            android:textSize="@dimen/_12ssp"            android:fontFamily="@font/sf_pro_text_bold"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtQuotePrice"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/listQuoteImages"            app:layout_constraintTop_toBottomOf="@+id/txtQuoteNumber"            app:layout_constraintEnd_toStartOf="@+id/txtResend"            app:layout_constraintBottom_toTopOf="@+id/txtCreateOrder"            android:text="@string/quote_price"            android:textColor="@android:color/black"            android:layout_marginStart="@dimen/_4sdp"            android:textSize="@dimen/_12ssp"            android:fontFamily="@font/sf_pro_text_regular"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtCreateOrder"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:text="@string/create_order"            android:textColor="@android:color/white"            android:textSize="@dimen/_10ssp"            android:fontFamily="@font/sf_pro_text_bold"            app:layout_constraintEnd_toStartOf="@+id/txtResend"            android:foreground="?android:attr/selectableItemBackground"            app:layout_constraintStart_toEndOf="@+id/listQuoteImages"            app:layout_constraintBottom_toBottomOf="@+id/listQuoteImages"            android:paddingStart="@dimen/_12sdp"            android:paddingEnd="@dimen/_12sdp"            android:paddingTop="@dimen/_6sdp"            android:paddingBottom="@dimen/_6sdp"            android:layout_marginEnd="@dimen/_12sdp"            android:drawableStart="@drawable/ic_create_order"            android:drawablePadding="@dimen/_4sdp"            android:background="@drawable/bg_green_rounded"            app:layout_constraintTop_toBottomOf="@+id/txtQuotePrice"/>    <androidx.constraintlayout.widget.ConstraintLayout            android:id="@+id/layoutStatus"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="@dimen/_4sdp"            app:layout_constraintStart_toEndOf="@+id/txtAddress"            app:layout_constraintBottom_toTopOf="@+id/txtView"            app:layout_constraintEnd_toEndOf="@+id/txtResend"            android:layout_marginBottom="@dimen/_2sdp"            app:layout_constraintTop_toTopOf="parent"            android:background="@drawable/bg_grey_line_rounded_corner">        <androidx.appcompat.widget.AppCompatTextView                android:id="@+id/txtStatusLabel"                android:layout_width="match_parent"                android:layout_height="wrap_content"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent"                android:text="@string/status"                android:textColor="@color/theme_green"                android:paddingStart="@dimen/_16sdp"                android:paddingEnd="@dimen/_16sdp"                android:paddingTop="@dimen/_4sdp"                android:paddingBottom="@dimen/_4sdp"                android:gravity="center"                android:fontFamily="@font/sf_pro_text_bold"/>        <View                android:id="@+id/view"                android:layout_width="match_parent"                android:layout_height="2px"                android:background="@color/grey"                app:layout_constraintTop_toBottomOf="@+id/txtStatusLabel"/>        <androidx.appcompat.widget.AppCompatTextView                android:id="@+id/txtStatus"                android:layout_width="match_parent"                android:layout_height="wrap_content"                app:layout_constraintStart_toStartOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toBottomOf="@+id/view"                tools:text="@string/status"                android:textColor="@android:color/black"                android:paddingStart="@dimen/_16sdp"                android:paddingEnd="@dimen/_16sdp"                android:paddingTop="@dimen/_4sdp"                android:paddingBottom="@dimen/_4sdp"                android:gravity="center_horizontal"                android:minLines="2"                android:fontFamily="@font/sf_pro_text_regular"/>    </androidx.constraintlayout.widget.ConstraintLayout>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtView"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/txtAddress"            app:layout_constraintEnd_toEndOf="@+id/txtResend"            app:layout_constraintBottom_toTopOf="@+id/txtDetails"            android:text="@string/view"            android:textColor="@color/theme_green"            android:paddingStart="@dimen/_20sdp"            android:paddingEnd="@dimen/_20sdp"            android:paddingTop="@dimen/_4sdp"            android:paddingBottom="@dimen/_4sdp"            android:textSize="@dimen/_10ssp"            android:gravity="center"            android:layout_marginStart="@dimen/_8sdp"            android:layout_marginBottom="@dimen/_2sdp"            android:fontFamily="@font/sf_pro_text_bold"            android:background="@drawable/bg_green_line_rounded"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtDetails"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/txtAddress"            app:layout_constraintEnd_toEndOf="@+id/txtResend"            app:layout_constraintBottom_toTopOf="@+id/txtEdit"            android:text="@string/details"            android:textColor="@color/theme_green"            android:paddingStart="@dimen/_20sdp"            android:paddingEnd="@dimen/_20sdp"            android:paddingTop="@dimen/_4sdp"            android:paddingBottom="@dimen/_4sdp"            android:textSize="@dimen/_12ssp"            android:gravity="center"            android:visibility="gone"            android:layout_marginStart="@dimen/_8sdp"            android:layout_marginBottom="@dimen/_2sdp"            android:fontFamily="@font/sf_pro_text_bold"            android:background="@drawable/bg_green_line_rounded"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtEdit"            android:layout_width="0dp"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/txtAddress"            app:layout_constraintEnd_toEndOf="@+id/txtResend"            app:layout_constraintBottom_toTopOf="@+id/txtResend"            android:text="@string/duplicate"            android:textColor="@color/theme_green"            android:paddingStart="@dimen/_20sdp"            android:paddingEnd="@dimen/_20sdp"            android:paddingTop="@dimen/_4sdp"            android:paddingBottom="@dimen/_4sdp"            android:textSize="@dimen/_12ssp"            android:gravity="center"            android:layout_marginStart="@dimen/_8sdp"            android:layout_marginTop="@dimen/_2sdp"            android:fontFamily="@font/sf_pro_text_bold"            android:background="@drawable/bg_green_line_rounded"/>    <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtResend"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintStart_toEndOf="@+id/txtAddress"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toBottomOf="@+id/txtEdit"            android:text="@string/resend"            android:textColor="@color/theme_green"            android:paddingStart="@dimen/_20sdp"            android:paddingEnd="@dimen/_20sdp"            android:paddingTop="@dimen/_4sdp"            android:paddingBottom="@dimen/_4sdp"            android:layout_marginStart="@dimen/_8sdp"            android:layout_marginTop="@dimen/_2sdp"            android:gravity="center"            android:textSize="@dimen/_12ssp"            android:fontFamily="@font/sf_pro_text_bold"            android:background="@drawable/bg_green_line_rounded"/></androidx.constraintlayout.widget.ConstraintLayout>