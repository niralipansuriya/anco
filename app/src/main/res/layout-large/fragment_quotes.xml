<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@android:color/white"    tools:context="com.app.ancoturf.presentation.home.shop.ShopFragment">    <include        layout="@layout/header"        android:id="@+id/header"        android:layout_width="0dp"        android:layout_height="wrap_content"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"/>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/txtHeaderLabel"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="@string/quote_label"        android:textColor="@android:color/black"        android:textSize="@dimen/_24sdp"        android:fontFamily="@font/sf_pro_display_bold"        app:layout_constraintStart_toStartOf="parent"        android:layout_marginStart="@dimen/_8sdp"        app:layout_constraintTop_toBottomOf="@+id/header"/>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/txtAddQuote"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="@string/add_new_quote"        android:textColor="@android:color/white"        android:textSize="@dimen/_11ssp"        android:fontFamily="@font/sf_pro_text_bold"        android:foreground="?android:attr/selectableItemBackground"        app:layout_constraintEnd_toStartOf="@+id/imgFilter"        android:paddingStart="@dimen/_12sdp"        android:paddingEnd="@dimen/_12sdp"        android:paddingTop="@dimen/_8sdp"        android:paddingBottom="@dimen/_8sdp"        android:layout_marginEnd="@dimen/_12sdp"        android:visibility="visible"        android:background="@drawable/bg_green_rounded"        app:layout_constraintTop_toBottomOf="@+id/header"/>    <androidx.appcompat.widget.AppCompatImageView        android:id="@+id/imgFilter"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/header"        android:src="@drawable/ic_filter"        android:padding="@dimen/_4sdp"        android:layout_marginTop="@dimen/_2sdp"        android:layout_marginEnd="@dimen/_8sdp"/>    <View        android:id="@+id/view"        android:layout_width="match_parent"        android:layout_height="2px"        android:background="@color/grey"        android:layout_marginTop="@dimen/_8sdp"        app:layout_constraintTop_toBottomOf="@+id/txtHeaderLabel"/>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/txtSubTitle"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/view"        android:text="@string/recent_quotes"        android:textColor="@android:color/black"        android:textSize="@dimen/_13sdp"        android:visibility="gone"        android:layout_marginStart="@dimen/_8sdp"        android:layout_marginTop="@dimen/_8sdp"        android:fontFamily="@font/sf_pro_text_bold"/>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/txtShowAllQuotes"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintTop_toBottomOf="@+id/view"        app:layout_constraintEnd_toEndOf="parent"        android:text="@string/show_all"        android:textColor="@android:color/black"        android:textSize="@dimen/_12sdp"        android:drawablePadding="@dimen/_2sdp"        android:visibility="gone"        android:drawableEnd="@drawable/ic_fw_arrow"        android:layout_marginStart="@dimen/_8sdp"        android:layout_marginEnd="@dimen/_8sdp"        android:layout_marginTop="@dimen/_8sdp"        android:fontFamily="@font/sf_pro_text_regular"/>    <!-- <androidx.core.widget.NestedScrollView             android:id="@+id/nestedScroll"             android:layout_width="match_parent"             app:layout_constraintStart_toStartOf="parent"             android:fillViewport="true"             app:layout_constraintEnd_toEndOf="parent"             app:layout_constraintTop_toBottomOf="@+id/txtSubTitle"             app:layout_constraintBottom_toBottomOf="parent"             android:visibility="visible"             android:layout_height="0dp">         <androidx.constraintlayout.widget.ConstraintLayout                 android:layout_width="match_parent"                 android:layout_height="wrap_content">-->    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/listQuotes"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="@dimen/_4sdp"        app:layout_constraintTop_toBottomOf="@+id/txtSubTitle"        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"        android:orientation="vertical"            android:layout_marginBottom="@dimen/_20sdp"/>    <!--            </androidx.constraintlayout.widget.ConstraintLayout>        </androidx.core.widget.NestedScrollView>-->    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/txtNoQuotes"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/view"        app:layout_constraintBottom_toBottomOf="parent"        android:gravity="center"        android:text="@string/no_quote_found"        android:textSize="@dimen/_15ssp"        tools:visibility="visible"        android:visibility="gone"        android:textColor="@android:color/black"        app:fontFamily="@font/sf_pro_display_regular"/>    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/layoutNoQuotes"        android:layout_width="match_parent"        tools:visibility="visible"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/view"        app:layout_constraintBottom_toBottomOf="parent"        android:layout_height="0dp"        android:visibility="gone"        android:padding="@dimen/_8sdp">        <androidx.appcompat.widget.AppCompatImageView            android:layout_width="match_parent"            android:layout_height="match_parent"            android:src="@drawable/bg_quotes"            android:scaleType="fitXY"/>        <androidx.appcompat.widget.AppCompatImageView            android:id="@+id/imgShop"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="parent"            app:layout_constraintVertical_chainStyle="packed"            app:layout_constraintBottom_toTopOf="@+id/txtAddBusinessLabel"            android:src="@drawable/img_shop"/>        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtAddBusinessLabel"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toBottomOf="@+id/imgShop"            app:layout_constraintBottom_toTopOf="@+id/txtGetStarted"            android:layout_marginTop="@dimen/_16sdp"            android:gravity="center"            android:text="@string/add_business_label"            android:textSize="@dimen/_13ssp"            android:textColor="@android:color/black"            app:fontFamily="@font/sf_pro_text_bold"/>        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/txtGetStarted"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="@string/get_started"            android:textColor="@android:color/white"            android:textSize="@dimen/_11sdp"            android:fontFamily="@font/sf_pro_text_bold"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintEnd_toEndOf="parent"            android:foreground="?android:attr/selectableItemBackground"            android:paddingStart="@dimen/_20sdp"            android:paddingEnd="@dimen/_20sdp"            android:paddingTop="@dimen/_8sdp"            android:paddingBottom="@dimen/_8sdp"            android:layout_marginTop="@dimen/_16sdp"            android:background="@drawable/bg_green_rounded"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintTop_toBottomOf="@+id/txtAddBusinessLabel"/>    </androidx.constraintlayout.widget.ConstraintLayout></androidx.constraintlayout.widget.ConstraintLayout>